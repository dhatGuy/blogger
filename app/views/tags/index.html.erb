<h1>All Articles</h1>

<ul id="tags">
    <% @tag.each do |t| %>
        <li>
        <%= link_to t.name, article_path(t) %>
        <%= link_to "delete", tag_path(t), method: :delete, data: {confirm: "Are you sure?"} %>
        </li>
    <% end %>
    <br>
    <%= link_to " Back to Articles List", articles_path %>
</ul>